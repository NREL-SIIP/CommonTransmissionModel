    "title": "Generators API",
    "version": "1.0.0"
  },
  "host": "virtserver.swaggerhub.com",
  "basePath": "/test5228/Generators/1.0.0",
  "schemes": [
    "https"
  ],
  "paths": {
    "/components/generators": {
      "get": {
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "description": "pass an optional search string for looking up generators",
            "in": "query",
            "name": "searchString",
            "required": false,
            "type": "string"
          },
          {
            "description": "number of records to skip for pagination",
            "format": "int32",
            "in": "query",
            "minimum": 0,
            "name": "skip",
            "required": false,
            "type": "integer"
          },
          {
            "description": "maximum number of records to return",
            "format": "int32",
            "in": "query",
            "maximum": 50,
            "minimum": 0,
            "name": "limit",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "search results matching criteria",
            "schema": {
              "items": {
                "$ref": "#/definitions/Generator"
              },
              "type": "array"
            }
          },
          "400": {
            "description": "bad input parameter"
          }
        },
        "tags": [
          "developers"
        ],
        "description": "By passing in the appropriate options, you can search for\navailable generators in the system\n",
        "operationId": "listGenerators",
        "summary": "lists generators"
      },
      "post": {
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "description": "Generator to add",
            "in": "body",
            "name": "body",
            "schema": {
              "$ref": "#/definitions/Generator"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "generator created"
          },
          "400": {
            "description": "invalid input, object invalid"
          },
          "409": {
            "description": "an existing generator already exists"
          }
        },
        "tags": [
          "admins"
        ],
        "description": "Adds a generator to the system",
        "operationId": "addGenerator",
        "summary": "adds a generator"
      }
    }
  },
  "definitions": {
    "Generator": {
      "properties": {
        "bus": {
          "description": "connecting bus id",
          "example": 1,
          "type": "integer"
        },
        "cost_pg_model": {
          "description": "cost model type (1=pwl, 2=polynomial)",
          "example": 1,
          "type": "integer"
        },
        "cost_pg_parameters": {
          "description": "either points for a pwl cost or polynomial coeffents (from lowest degree first)",
          "example": [
            1.1
          ],
          "items": {
            "type": "number"
          },
          "type": "array"
        },
        "cost_start_cold": {
          "description": "cost in dollars of starting a unit",
          "example": 1,
          "type": "number"
        },
        "cost_start_hot": {
          "description": "cost in dollars of starting a unit",
          "example": 1,
          "type": "number"
        },
        "cost_start_warm": {
          "description": "cost in dollars of starting a unit",
          "example": 1,
          "type": "number"
        },
        "down_time": {
          "description": "time down previously",
          "example": 1.1,
          "type": "number"
        },
        "fuel_type": {
          "description": "fuel type",
          "example": 1,
          "type": "integer"
        },
        "index": {
          "description": "a unique identifier for components",
          "example": 1,
          "type": "integer"
        },
        "max_run": {
          "description": "unit of time (hours)",
          "example": 0,
          "type": "number"
        },
        "min_down": {
          "description": "unit of time (hours)",
          "example": 0,
          "type": "number"
        },
        "min_run": {
          "description": "unit of time (hours)",
          "example": 0,
          "type": "number"
        },
        "must_run": {
          "description": "enum",
          "example": 1,
          "type": "integer"
        },
        "name": {
          "description": "a flexible name for components, non required to be unique",
          "example": "1",
          "type": "string"
        },
        "pg": {
          "description": "active/real power generation output",
          "example": 1.2,
          "type": "number"
        },
        "pg_0": {
          "description": "previous active power output",
          "example": 1,
          "type": "number"
        },
        "pg_lb": {
          "description": "minimum active power generation",
          "example": 1,
          "type": "number"
        },
        "pg_ub": {
          "description": "maximum active power generation",
          "example": 2,
          "type": "number"
        },
        "pgd_lb": {
          "description": "maximum active power increase per hour",
          "example": 1.1,
          "type": "number"
        },
        "pgd_ub": {
          "description": "maximum active power increase per hour",
          "example": 1.1,
          "type": "number"
        },
        "qg": {
          "description": "reactive/imaginary power generation output",
          "example": 1.1,
          "type": "number"
        },
        "qg_lb": {
          "description": "minimum active power generation",
          "example": 0,
          "type": "number"
        },
        "qg_ub": {
          "description": "maximum reactive power generation",
          "example": 0,
          "type": "number"
        },
        "reserve_agc": {
          "description": "1/0",
          "example": 1,
          "type": "boolean"
        },
        "reserve_agc_pg_lb": {
          "description": "1/0",
          "example": 0,
          "type": "number"
        },
        "reserve_agc_pg_ub": {
          "description": "1/0",
          "example": 0,
          "type": "number"
        },
        "service_on": {
          "description": "in service",
          "example": 1,
          "type": "boolean"
        },
        "status": {
          "description": "a 0/1 value indicating if the component should be omitted or not (0 => omitted)",
          "example": 1,
          "type": "integer"
        },
        "time_start_cold": {
          "description": "threshold of time (est. 16h+)",
          "example": 1,
          "type": "number"
        },
        "time_start_hot": {
          "description": "threshold of time (est. 9h)",
          "example": 1,
          "type": "number"
        },
        "time_start_warm": {
          "description": "threshold of time (est. 9-16h)",
          "example": 1,
          "type": "number"
        },
        "up_time": {
          "description": "time up previously",
          "example": 1.1,
          "type": "number"
        }
      },
      "required": [
        "bus",
        "index",
        "pg",
        "qg",
        "status"
      ],
      "type": "object"
    }
  },
  "tags": [
    {
      "description": "Secured Admin-only calls",
      "name": "admins"
    },
    {
      "description": "Operations available to regular developers",
      "name": "developers"
    }
  ],
